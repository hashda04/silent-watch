{
  "name": "silent-watch",
  "version": "0.1.0",
  "main": "dist/watcher.cjs.js",
  "module": "dist/watcher.esm.js",
  "unpkg": "dist/watcher.umd.js",
  "types": "dist/types/core/watcher.d.ts",
  "exports": {
    ".": {
      "import": "./dist/watcher.esm.js",
      "require": "./dist/watcher.cjs.js"
    }
  },
  "scripts": {
    "build": "tsup src/core/watcher.ts --format esm --dts --out-dir dist",
  "build:rollup": "rollup -c"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "silent failures",
    "ui monitoring",
    "workflow detection",
    "frontend tool"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
